<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
    <Appenders>
        <File name="File" fileName="log.txt">
            <PatternLayout pattern="[%d]  - %msg%n"/>
        </File>
    </Appenders>
    
    <Loggers>
        <Root level="all">
            <AppenderRef ref="File"/>
        </Root>
    </Loggers>
</Configuration>